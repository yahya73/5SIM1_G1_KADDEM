
pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage('Git') {
            steps {
                git branch: 'salim', changelog: false, poll: false, url: 'https://github.com/yahya73/KADDEM_DEVOPS.git'
            }
        }
                 stage('Testing Maven') {
                                      steps {
                                          sh 'mvn -version'
                                      }
                                  }

             stage('Build') {
                 steps {
                     sh 'mvn clean install -DskipITs'
                 }
             }
             stage ('SonarQube') {

                                 steps {

                                     sh 'mvn sonar:sonar -Dsonar.login=admin -Dsonar.password=KairouanMilan123$ -Dmaven.test.skip=true';

                                 }

                             }
                             stage ('Nexus') {
                                 steps {
                                     sh 'mvn deploy -DskipTests'
                                       }
                             }
       }

}